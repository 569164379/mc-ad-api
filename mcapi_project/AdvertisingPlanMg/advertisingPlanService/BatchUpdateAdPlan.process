    </pd:returnBindings>
    <pd:endName>End</pd:endName>
    <pd:endType>
        <xsd:element name="responseJsonStr" type="xsd:string"/>
    </pd:endType>
    <pd:endX>1153</pd:endX>
    <pd:endY>304</pd:endY>
    <pd:errorSchemas/>
