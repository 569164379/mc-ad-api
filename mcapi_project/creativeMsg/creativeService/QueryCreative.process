                                </pfx3:creativeLevel>
                            </xsl:if>
                            <xsl:if test="pfx3:creativeCreated">
                                <pfx3:creativeCreated>
                                    <xsl:value-of select="pfx3:creativeCreated"/>
                                </pfx3:creativeCreated>
                            </xsl:if>
                            <xsl:if test="pfx3:creativeModified">
                                <pfx3:creativeModified>
                                    <xsl:value-of select="pfx3:creativeModified"/>
                                </pfx3:creativeModified>
                            </xsl:if>
