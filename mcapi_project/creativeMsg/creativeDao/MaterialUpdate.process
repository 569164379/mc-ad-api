    <pd:activity name="SQL Direct">
        <pd:type>com.tibco.plugin.jdbc.JDBCGeneralActivity</pd:type>
        <pd:resourceType>ae.activities.JDBCGeneralActivity</pd:resourceType>
        <pd:x>298</pd:x>
        <pd:y>101</pd:y>
        <config>
            <timeout>10</timeout>
            <commit>false</commit>
            <maxRows>100</maxRows>
            <jdbcSharedConfig>/shareResources/connections/JDBC Connection.sharedjdbc</jdbcSharedConfig>
        </config>
        <pd:inputBindings>
            <jdbcGeneralActivityInput>
                <statement>
                    <xsl:value-of select="concat(&quot;update tab_material set MATERIAL_TYPE=MATERIAL_TYPE&quot;,&#xA;if (exists($Start/pfx2:materialObj/pfx2:creativeId) and $Start/pfx2:materialObj/pfx2:creativeId !=&quot;&quot;) then concat(&quot; ,CREATIVE_ID=&quot;, $Start/pfx2:materialObj/pfx2:creativeId)  else  &quot;&quot;,&#xA;if (exists($Start/pfx2:materialObj/pfx2:materialIsdel) and $Start/pfx2:materialObj/pfx2:materialIsdel !=&quot;&quot;) then concat(&quot; ,MATERIAL_ISDEL=&quot;, $Start/pfx2:materialObj/pfx2:materialIsdel)  else  &quot;&quot;,&#xA;if (exists($Start/pfx2:materialObj/pfx2:materialUrl) and $Start/pfx2:materialObj/pfx2:materialUrl !=&quot;&quot;) then concat(&quot; ,MATERIAL_URL='&quot;, $Start/pfx2:materialObj/pfx2:materialUrl,&quot;'&quot;)  else  &quot;&quot;,&#xA;if (exists($Start/pfx2:materialObj/pfx2:materialModified) and $Start/pfx2:materialObj/pfx2:materialModified !=&quot;&quot;) then concat(&quot; ,MATERIAL_MODIFIED='&quot;, $Start/pfx2:materialObj/pfx2:materialModified,&quot;'&quot;)  else  &quot;&quot;,&#xA;&quot;  where MATERIAL_ID=&quot;,$Start/pfx2:materialObj/pfx2:materialId)"/>
                </statement>
            </jdbcGeneralActivityInput>
        </pd:inputBindings>
    </pd:activity>
    <pd:activity name="Catch">
        <pd:type>com.tibco.pe.core.CatchActivity</pd:type>
        <pd:resourceType>ae.activities.catch</pd:resourceType>
        <pd:x>704</pd:x>
        <pd:y>58</pd:y>
        <pd:handler>true</pd:handler>
        <config>
            <catchAll>true</catchAll>
        </config>
        <pd:inputBindings/>
    </pd:activity>
    <pd:activity name="Rethrow">
        <pd:type>com.tibco.pe.core.RethrowActivity</pd:type>
        <pd:resourceType>ae.activities.rethrow</pd:resourceType>
        <pd:x>718</pd:x>
        <pd:y>197</pd:y>
        <config/>
