            <maxRows>100</maxRows>
            <jdbcSharedConfig>/shareResources/connections/JDBC Connection.sharedjdbc</jdbcSharedConfig>
        </config>
        <pd:inputBindings>
            <jdbcGeneralActivityInput>
                <statement>
                    <xsl:value-of select="concat(&quot;SELECT bcl.CREATIVELOG_RESULT creativeLogRsName from tab_creative bc INNER JOIN tab_creative_log bcl on  bc.CREATIVE_ID=bcl.CREATIVE_ID&quot;, &#xA;&quot;   where bc.CREATIVE_ID=&quot;, $Start/root/creativeId,&#xA;&quot; and bc.CREATIVE_STATUS=13 order by bcl.CREATIVELOG_DATE desc limit 1&quot;&#xA;)"/>
                </statement>
            </jdbcGeneralActivityInput>
        </pd:inputBindings>
